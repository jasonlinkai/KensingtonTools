{
  "name": "kensington-tools",
  "version": "1.0.0",
  "description": "A productivity application built with Electron and React",
  "main": "dist/main/main.js",
  "scripts": {
    "typescript:watch": "tsc -w",
    "typescript:build": "tsc",
    "vite:watch": "vite",
    "vite:build": "vite build",
    "electron:start": "cross-env NODE_ENV=development electron .",
    "electron:build:win": "electron-builder --win --config electron-builder-win.js",
    "electron:build:mac": "electron-builder --mac --config electron-builder-mac.js",
    "start:watch": "concurrently -k \"npm run typescript:watch\" \"npm run vite:watch\"",
    "dev": "concurrently -k \"npm run start:watch\" \"wait-on tcp:5173 && npm run electron:start\"",
    "build:win": "npm run electron:build:win",
    "build:mac": "npm run electron:build:mac",
    "build-all:win": "rimraf dist && rimraf release && npm run typescript:build && npm run vite:build && npm run build:win",
    "build-all:mac": "rimraf dist && rimraf release && npm run typescript:build && npm run vite:build && npm run build:mac",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron/notarize": "^3.0.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-builder-notarize": "^1.5.2",
    "postcss": "^8.5.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-electron": "^0.15.6",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/react-router-dom": "^5.3.3",
    "electron-log": "^5.3.4",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "lingo.dev": "^0.87.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^7.5.2",
    "tailwindcss": "^3.4.1",
    "xlsx": "^0.18.5"
  }
}
